{
  "require": {
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "Core\\": "app/Core/",
      "Http\\Controllers\\": "app/Http/Controllers/",
      "Models\\": "app/Models/",
      "Schema\\": "app/Schema/",
      "Views\\": "app/Views/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "pestphp/pest": "^3.8"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
